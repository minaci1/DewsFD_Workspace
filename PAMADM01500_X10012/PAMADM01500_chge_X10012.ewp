    <javascript id="8b1c795e-7723-4753-80f7-f72da2435ec1"><![CDATA[function () {
  if (self.inputPanel.validate({ tooltip: true, message: '필수 항목을 입력해 주십시요.' })) {
    var chgeCd = self.REQ_FG.value();
    var checkedRows = self.grid.getCheckedRows();
    if (checkedRows.length < 1) {
      dews.ui.snackbar.error("선택된 신청이 없습니다.");
      return false;
    }
    if (chgeCd == "2") {
      var stayTermStartDt = self.STAY_TERM_START_DT.value();
      var stayTermEndDt = self.STAY_TERM_END_DT.value();
      if (stayTermStartDt && stayTermEndDt) {
        if (stayTermStartDt > stayTermEndDt) {
          dews.ui.snackbar.error("출입시작일이 출입종료일보다 빠릅니다.");
          return false;
        }
      }
      for (var i = 0; i < checkedRows.length; i++) {
        if (checkedRows[i].ST_FG != "1") {
          dews.ui.snackbar.error("교부중인 카드가 아닐 경우 연장할 수 없습니다.");
          return false;
        }
      }
    } else if (chgeCd == "3") {
      for (var i = 0; i < checkedRows.length; i++) {
        if (checkedRows[i].ST_FG != "1") {
          dews.ui.snackbar.error("교부중인 카드가 아닐 경우 회수할 수 없습니다.");
          return false;
        }
      }
    } else if (chgeCd == "4") {
      for (var i = 0; i < checkedRows.length; i++) {
        if (checkedRows[i].ST_FG != "1" && checkedRows[i].ST_FG != "0") {
          dews.ui.snackbar.error("미교부나 교부중인 카드가 아닐 경우 불량처리할 수 없습니다.");
          return false;
        }
      }
    } else if (chgeCd == "5") {
      for (var i = 0; i < checkedRows.length; i++) {
        if (checkedRows[i].ST_FG != "4") {
          dews.ui.snackbar.error("불량처리 되지 않은 카드가 아닐경우 폐기 할 수 없습니다.");
          return false;
        }
      }
    } else if (chgeCd == "7") {
      if (checkedRows.length > 1) {
        dews.ui.snackbar.error("교환처리는 한개의 카드만 선택 가능합니다.");
        return false;
      }
      for (var i = 0; i < checkedRows.length; i++) {
        if (checkedRows[i].ST_FG != "1") {
          dews.ui.snackbar.error("교부중인 카드가 아닐 경우 교환할 수 없습니다.");
          return false;
        }
      }
    } else if (chgeCd == "8") {
    }
    dews.confirm("저장하시겠습니까?").yes(function () {
      dews.api.post(dews.url.getApiUrl('HR', 'PamadmServiceX10012', 'pamadm01500_save_chge'), {
        async: false,
        data: {
          dataSource: JSON.stringify(checkedRows),
          req_dt: new Date().toJSON(),
          req_fg: self.REQ_FG.value(),
          chge_reason_cd: self.CHGE_REASON_CD.value(),
          card_no2: self.CARD_NO2.code(),
          card_nm2: self.CARD_NO2.text(),
          stay_term_start_dt: self.STAY_TERM_START_DT.value(),
          stay_term_end_dt: self.STAY_TERM_END_DT.value(),
          loss_dt:self.LOSS_DT.value()
        }
      }).done(function (data) {
        dews.ui.snackbar.ok('저장되었습니다.');
        self.ok();
      }).fail(function (xhr, status, error) {
        dews.ui.snackbar.error(error);
      });
    }).no(function () {
      dews.ui.snackbar.info("취소되었습니다.");
    });
  }
}]]></javascript>
