    <javascript id="cc2af06d-6499-434e-bd27-4152ec832fd1"><![CDATA[function() {
  var dirtyCountgrid1 = self.dataSourceUN_grid1_M.getDirtyDataCount();
  var dirtyCountgrid3 = self.dataSourceUN_grid3_M.getDirtyDataCount();
  var dirtyCountgridDetail1 = self.dataSourceUN_gridDetail1_M.getDirtyDataCount();
  if (dirtyCountgrid1 == 0 && dirtyCountgrid3 == 0 && dirtyCountgridDetail1 == 0) {
    dews.alert('변경된 데이터가 없습니다.');
  }
  if (self.grid1.validate().result == false) {
    dews.alert('그리드 grid1에 필수 값이 입력되지 않은 항목이 있습니다.');
  }
  dews.confirm('수정 된 데이터를 저장 하시겠습니까?', 'question').yes(function () {
    dews.api.post(dews.url.getApiUrl('CI', 'CiUsrdeftblService', 'usrdeftbl_data_milti_save'), {
      async: true,
      data: {
        module_cd: dews.ui.page.menu.module,
        Usrdeftbl_Data_Mst: JSON.stringify(self.dataSourceUN_grid1_M.getDirtyData()),
        Usrdeftbl_Data_Dtl: JSON.stringify(self.dataSourceUN_gridDetail1_M.getDirtyData())
      }
    }).done(function (data) {
      dews.ui.snackbar.ok('저장이 완료되었습니다.');
      self.dataSourceUN_grid1_M.read();
    }).fail(function () {
      dews.error('저장에 실패 했습니다.');
    });
  });
}
]]></javascript>
